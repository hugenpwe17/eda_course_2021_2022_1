module SS_TEST_vlg_vec_tst();
// constants                                           
// general purpose registers
reg clk;
reg load;
reg rst_n;
// wires                                               
wire [15:0] seq;
wire seq_jug;
wire [3:0] state;

wire sampler;                             

// assign statements (if any)                          
SS_TEST i1 (
// port map - connection between master ports and signals/registers   
	.clk(clk),
	.load(load),
	.rst_n(rst_n),
	.seq(seq),
	.seq_jug(seq_jug),
	.state(state)
);

// clk
always
begin
	clk = 1'b0;
	clk = #10000 1'b1;
	#10000;
end 

// load
initial
begin
	load = 1'b0;
	load = #40000 1'b1;
	load = #20000 1'b0;
end 

// rst_n
initial
begin
	rst_n = 1'b1;
end 

SS_TEST_vlg_sample_tst tb_sample (
	.clk(clk),
	.load(load),
	.rst_n(rst_n),
	.sampler_tx(sampler)
);

SS_TEST_vlg_check_tst tb_out(
	.seq(seq),
	.seq_jug(seq_jug),
	.state(state),
	.sampler_rx(sampler)
);
endmodule

