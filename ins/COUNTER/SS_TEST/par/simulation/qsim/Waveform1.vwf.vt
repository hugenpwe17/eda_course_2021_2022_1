`timescale 1 ps/ 1 ps
module SS_TEST_vlg_sample_tst(
	clk,
	load,
	rst_n,
	sampler_tx
);
input  clk;
input  load;
input  rst_n;
output sampler_tx;

reg sample;
time current_time;
always @(clk or load or rst_n)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
		if (sample === 1'bx)                          
			sample = 0;                               
		else                                          
			sample = ~sample;                         
	end										          
	current_time = $realtime;					      
 end                                                  
end                                                   

assign sampler_tx = sample;
endmodule

