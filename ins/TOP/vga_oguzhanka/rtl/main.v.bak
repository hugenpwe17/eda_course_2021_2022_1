module main (
    input wire clk, 
    output wire VGA_clk,

    output wire [9:0] xCount,
    output wire [9:0] yCount,
    output wire [7:0] display,
    output wire waveform,
    
    output wire VGA_hSync,
    output wire VGA_vSync,
    output wire blank_n,
    
    output wire [1:0] Red_1,
    output wire [1:0] Blue_1,
    output wire [1:0] Green_1
);
    
    vga_driver u_vga_driver(
        .clk       ( clk       ),
        .VGA_clk   ( VGA_clk   ),
        .xCount    ( xCount    ),
        .yCount    ( yCount    ),
        .display   ( display   ),
        .waveform  ( waveform  ),
        .VGA_hSync ( VGA_hSync ),
        .VGA_vSync ( VGA_vSync ),
        .blank_n   ( blank_n   ),
        .Red_1     ( Red_1     ),
        .Blue_1    ( Blue_1    ),
        .Green_1   ( Green_1   )
    );

endmodule